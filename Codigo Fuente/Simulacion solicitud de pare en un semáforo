input.onButtonPressed(Button.A, function () {   
    basic.showNumber(5)
    basic.pause(1000)  
    basic.showNumber(4)
    basic.pause(1000)
    basic.showNumber(3)
    basic.pause(1000)
    basic.showNumber(2)
    basic.pause(1000)
    basic.showNumber(1)
    basic.showIcon(IconNames.No)  
    basic.pause(30000)
    basic.showIcon(IconNames.Yes)   
basic.showIcon(IconNames.Yes)
